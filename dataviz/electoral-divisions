    <script>
        $(document).ready(function(){

            // display the main UI panels
            aa.populationUi.displayUiPanels();        

            aa.columnchart('#population-chart', aa.population, 
                {
                    width: $('#population-chart').width(), 
                    height: $('#population-chart').height()
                }
            );

 
            // if county view => display county level stats &
            // high level company chart / refine map
            
                $.get('data/2011-county-population.json', function(data){

                    data = _.sortBy(data, function(d){ return +d.total_count; });
                    data = data.reverse();
                    aa.barchart('#county-populations', data, 
                    {
                        width: $('#county-populations').width(), 
                        height: $('#county-populations').height(),
                        hide_y_axis: true,
                        margin: {
                            left: 0,
                            right: 20,
                            top: 5,
                            bottom: 0
                        }
                    });
                }, 'json');


                            

            
        });
    </script>


