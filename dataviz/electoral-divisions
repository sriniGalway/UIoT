    <script>

        aa.areaId = null;
        aa.visType = 'electoral-division';

                    var map = new L.Map('map', {
                center: new L.LatLng(53.408, -9.128),
                zoom: 9,
                scrollWheelZoom: false,
            });

            var showPopup = true;
                
    </script>
    
    <script>
        L.tileLayer('http://{s}.tile.openstreetmap.se/hydda/full/{z}/{x}/{y}.png', {
            minZoom: 8,
            maxZoom: 15,
            noWrap: true,
            attribution: 'Tiles courtesy of <a href="http://openstreetmap.se/" target="_blank">OpenStreetMap Sweden</a> &mdash; Map data &copy; <a href="http://openstreetmap.org">OpenStreetMap</a> contributors, <a href="http://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>'
        }).addTo(map);

        $.get('/data/galway-electoral-divisions-company.json', function(data){
            
            aa.geoJson = data;
            
            // Update map UI with commercial layer
            // aa.populationUi.addCompanyCountLayer();
            
            aa.populationUi.addPopulationLayer();
            $('#industry-map-refine').hide();

            
            // Check if we have something to use for vacancy rates (Municip. Districts
            // don't have this data in the geojson file)
            if(typeof aa.geoJson.features[0].properties.PCVAC2011 !== 'undefined'){

                // initialise the HBS housing stock template
                aa.populationUi.displayHousingStockTemplate();

                // pull what's needed for housing stock charting
                aa.extractedHousing = _.map(aa.geoJson.features, 
                        function(features){ return { 
                            HS2011: features.properties.HS2011, 
                            PPOCC2011: features.properties.PPOCC2011, 
                            UNOCC2011: features.properties.UNOCC2011, 
                            VACANT2011: features.properties.VACANT2011, 
                            EDNAME: features.properties.EDNAME, 
                            CSOED: features.properties.CSOED,
                            PCFULL2011: (100 - features.properties.PCVAC2011),
                            PCVAC2011: +features.properties.PCVAC2011,
                        };
                    }
                );

                // Display housing panel
                aa.populationUi.displayHousingStockChart( aa.extractedHousing );
            }

        },'json');    
        
    </script>

